{
    "name": "metronic",
    "theme": "metronic",
    "desc": "Metronic build config file for Gulp",
    "version": "7.2.7",
    "config": {
        "debug": false,
        "compile": {
            "rtl": {
                "enabled": false,
                "skip": [
                    "select2",
                    "socicon",
                    "line-awesome",
                    "flaticon",
                    "flaticon2",
                    "fontawesome5",
                    "jstree",
                    "bootstrap-datepicker",
                    "bootstrap-switch",
                    "tempusdominus-bootstrap-4",
                    "nouislider",
                    "tinymce",
                    "sweetalert2",
                    "jquery-mask-plugin"
                ]
            },
            "jsMinify": false,
            "cssMinify": false,
            "jsSourcemaps": false,
            "cssSourcemaps": false
        },
        "path": {
            "src": "./../{demo}/src",
            "node_modules": "./node_modules"
        },
        "dist": [
            "./../{demo}/dist/assets"
        ]
    },
    "build": {
        "main": {
            "src": {
                "styles": [
                    "{$config.path.src}/sass/style.scss"
                ],
                "scripts": [
                    "{$config.path.src}/js/components/**/*.js",
                    "{$config.path.src}/js/layout/**/*.js"
                ]
            },
            "bundle": {
                "styles": "{$config.dist}/css/style.bundle.css",
                "scripts": "{$config.dist}/js/scripts.bundle.js"
            }
        },
        "vendors": {
